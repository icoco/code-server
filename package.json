{
  "name": "code-server",
  "version": "1.0.1",
  "description": "Multiplayer code editor system",
  "forked": "https://github.com/vizhub-core/vzcode",
  "main": "src/index.ts",
  "type": "module",
  "bin": "dist_server/server/index.js",
  "files": [
    "dist/**",
    "src/**"
  ],
  "scripts": {
    "test": "vitest run",
    "test-interactive": "cd test/sampleDirectories/kitchenSink && node ../../../src/server/index.js",
    "prettier": "prettier {*.*,**/*.*} --write",
    "tsc": "tsc --noEmit",
    "dev": "concurrently \"npm run test-interactive\" \"vite\"",
    "build": "vite build",
    "build-release": "vite build --mode release",
    "preview": "concurrently \"npm run test-interactive\" \"vite preview\"",
    "prepublishOnly": "npm run build-release",
   
    "start-server-max": "cross-env NODE_OPTIONS=--max_old_space_size=8096 & node --max-old-space-size=8096  src/server/index.js dev --space=./space --site=./dist",

    "start-server": "node src/server/index.js dev --space=./space --site=./dist ",

    "build-server": "npx babel ./src --out-dir dist_server",
    "pkg": "pkg . ",
    "pkg-pack": "copyfiles ./dist/* ./dist/**/* ./dist_server_bin  & copyfiles ./space/* ./dist_server_bin ",
    "pkg-go": "concurrently \"npm run pkg\" \"npm run pkg-pack\""
  },
  "pkg": {
    "scripts": "dist_server/server/index.js",
    "assets": [],
    "outputPath": "dist_server_bin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vizhub-core/vzcode.git"
  },
  "keywords": [
    "CodeServer",
    "Code",
    "Editor",
    "CodeMirror",
    "Collaboration",
    "Development",
    "IDE",
    "JavaScript",
    "Data",
    "Visualization"
  ],
  "contributors": [
    {
      "name": "VZCode",
      "url": "https://github.com/vizhub-core/vzcode"
    },
    {
      "name": "icoco",
      "url": "https://github.com/icoco/code-server"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icoco/code-server/issues"
  },
  "homepage": "https://github.com/icoco/code-server#readme",
